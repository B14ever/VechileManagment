{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "zeroConfig": true,
          "buildCommand": "tsc --noEmitOnError && vite build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  